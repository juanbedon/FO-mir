<div class="container">
	<div class="row">
		<div class="col-sm-12">
			<h1>Questions</h1>
			<table class="table">
				<tr>
					<th>Id</th>
					<th>Title</th>
					<th>Description</th>
				</tr>
				<% @questions.each do |question| %>
					<tr>
						<td><%= question.id %></td>
						<td><%= question.title %></td>
						<td><%= question.description %></td>
					</tr>
				<% end %>
			</table>
		</div>
	</div>
</div>